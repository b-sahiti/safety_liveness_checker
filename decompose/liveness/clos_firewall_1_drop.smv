MODULE R0()
VAR
	active:boolean;
ASSIGN
	init(active) := TRUE;

MODULE R1(r2,r3)
VAR
	active:boolean;
ASSIGN
	init(active) := TRUE;
	next(r2.active) := case
		self.active : FALSE;
		TRUE : r2.active;
		esac;
	next(r3.active) := case
		self.active : TRUE;
		TRUE : r3.active;
		esac;

MODULE R2(r3)
VAR
	active:boolean;
ASSIGN
	init(active):= FALSE;
	next(r3.active) := case
		self.active : FALSE;
		TRUE : r3.active;
		esac;
	next(self.active) := case
		self.active : TRUE;
		TRUE : self.active;
		esac;

MODULE R3(r4,r5)
VAR
	active:boolean;
ASSIGN
	init(active) := TRUE;
	next(r4.active) := case
		self.active : FALSE;
		TRUE : r4.active;
		esac;
	next(r5.active) := case
		self.active : TRUE;
		TRUE : r5.active;
		esac;

MODULE R4(r5)
VAR
	active:boolean;
ASSIGN
	init(active):= FALSE;
	next(r5.active) := case
		self.active : FALSE;
		TRUE : r5.active;
		esac;
	next(self.active) := case
		self.active : TRUE;
		TRUE : self.active;
		esac;

