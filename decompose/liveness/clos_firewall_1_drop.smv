MODULE main
VAR
	r0 : process R0();
	r1 : process R1(r2,r3);
	r2 : process R2(r3);
	r3 : process R3(r4,r5);
	r4 : process R4(r5);
	r5 : process R5(r6,r7);
	r6 : process R6(r7);
	r7 : process R7();
SPEC r0.active=TRUE&TRUE&FALSE|!(r0.active=TRUE&TRUE)&TRUE